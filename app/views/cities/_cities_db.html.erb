    <table class = "table table-striped">
    <thead class = "thead-dark">
        <th>Id</th>
        <th>Name</th>
        <th>Country</th>
    </thead>
    <tbody>
        <% @records_array.each do |city| %>
            <tr>
            <td>
                <%= city["id"] %>
            </td>
            <td>
                <%= city["name"] %>
            </td>
            <td>
                <%= city["country_id"] %>
            </td>
        <% end %>
    </tbody>
    </table>